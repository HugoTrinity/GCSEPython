import random as r

num = r.randint(1, 5)
guess = int(input("Enter a number between 1 and 5: "))

if guess == num:
    print("well done")
elif guess > num:
    print("too high")
    guess = int(input("Enter a number between 1 and 5: "))
    if guess == num:
        print("correct")
elif guess < num:
    print("too low")
    guess = int(input("Enter a number between 1 and 5: "))
    if guess == num:
        print("correct")
    
