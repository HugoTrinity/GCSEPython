EmptyArray = [[0 for column in range(0, 5)] for row in range(0, 5)]
for i in range(0, 5):
    print(EmptyArray[i])

print("You can change a specific location")
x = int(input("What column coordinate: "))
y = int(input("What row coordinate: "))
change = int(input("What do you want to change it to: "))
EmptyArray[y-1][x-1] = change

for i in range(0, 5):
    print(EmptyArray[i])
